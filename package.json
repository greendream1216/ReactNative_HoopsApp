{
  "name": "Hoops",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "postinstall": "rndebugger-open",
    "lint": "eslint src || :",
    "log-android": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "clean-app": "npm cache clean && watchman watch-del-all && rm -fr $TMPDIR/react-*",
    "clean-gradle": "rm -rf $HOME/.gradle/caches/",
    "android-setup-port": "adb reverse tcp:8081 tcp:8081",
    "lint-fix": "eslint -c .eslintrc src --fix"
  },
  "dependencies": {
    "bugsnag-react-native": "^1.2.4",
    "color": "^0.11.3",
    "elasticsearch": "^11.0.1",
    "firebase": "^3.6.5",
    "moment": "^2.15.0",
    "qs": "^6.2.1",
    "react": "15.4.1",
    "react-native": "^0.39.2",
    "react-native-fbsdk": "^0.4.0",
    "react-native-fcm": "^2.5.6",
    "react-native-fetch-blob": "0.10.1-beta.1",
    "react-native-google-places-autocomplete": "^1.2.6",
    "react-native-i18n": "0.0.8",
    "react-native-keyboard-spacer": "^0.3.0",
    "react-native-maps": "^0.12.4",
    "react-native-side-menu": "^0.20.0",
    "react-native-swiper": "^1.5.4",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.6.1",
    "redux-persist": "^4.0.1",
    "redux-thunk": "^2.1.0",
    "url": "^0.11.0"
  },
  "rnpm": {
    "assets": [
      "src/styles/resources/fonts"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "react-native-debugger-open": "^0.3.2",
    "remote-redux-devtools": "^0.5.7",
    "remote-redux-devtools-on-debugger": "^0.7.0"
  }
}
